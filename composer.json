{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-intl": "*",
        "doctrine/dbal": "^3",
        "guzzlehttp/guzzle": "^7",
        "kcs/class-finder": "^0",
        "laravel/framework": "^9",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2",
        "livewire/livewire": "^2",
        "outhebox/blade-flags": "^1",
        "rawilk/laravel-settings": "^2",
        "socialiteproviders/google": "^4.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "laravel/pint": "^1",
        "nunomaduro/collision": "^6",
        "spatie/laravel-ignition": "^1",
        "friendsofphp/php-cs-fixer": "^3",
        "laravel-lang/attributes": "^2.0",
        "laravel-lang/lang": "^12.0",
        "laravel-lang/publisher": "^14.0",
        "barryvdh/laravel-debugbar": "dev-master",
        "thiagocordeiro/laravel-translator": "^1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan storage:link",
            "@php artisan vendor:publish --tag=blade-flags --force"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
